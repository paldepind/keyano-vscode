{
  "name": "keyano",
  "displayName": "Keyano",
  "description": "Edit code with you keyboard.",
  "version": "0.0.1",
  "publisher": "paldepind",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Keyano configuration",
      "properties": {
        "keyano.keyboardLayout": {
          "type": "string",
          "enum": [
            "qwerty",
            "colemak"
          ],
          "default": "qwerty",
          "description": "The keyboard layout that should be used to correctly place positional bindings."
        }
      }
    },
    "keybindings": [
      {
        "key": "Escape",
        "command": "keyano.escape"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "install-extension": "rsync -r . ~/.vscode/extensions/keyano-vscode --exclude node_modules/.bin --exclude .vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.10.1",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.5.1",
    "js-yaml": "^3.10.0",
    "mocha": "^4.0.1",
    "typescript": "^2.6.2",
    "vscode": "^1.1.10"
  }
}
